# AWS CDK Configuration
# For development deployment (default)
CDK_DEFAULT_ACCOUNT=your-dev-aws-account-id
CDK_DEFAULT_REGION=eu-central-1

# For production deployment (required when CDK_ENVIRONMENT=prod)
# Set this when deploying to production
PROD_AWS_ACCOUNT=your-prod-aws-account-id

# SAMS API Configuration
SAMS_API_KEY=your-sams-api-key
SAMS_SERVER=your-sams-server-url
