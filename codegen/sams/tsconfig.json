{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "outDir": "dist",
    "composite": true,
    "rootDir": "generated",
    "skipLibCheck": true
  },
  "include": ["generated/**/*"]
}
